{
  "name": "elon_cx",
  "version": "1.0.1",
  "description": "elon's fist project",
  "main": "Gruntfile.js",
  "keywords": [
    "ccloud"
  ],
  "author": "elon",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://elon12345@bitbucket.org/kevin01/c_cloud_front_end.git"
  },
  "private": true,
  "scripts": {
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=/Users/dev_mac/chromedata/",
    "dev": "webpack-dev-server -d --progress",
    "build": "rimraf backup/otherapp/ccloud && webpack -d --progress",
    "build:prod": "rimraf backup/otherapp/ccloud && webpack -p --progress --env.BUILD_MODE=prod",
    "build:cordova": "rimraf cordova/www && webpack -d --progress --env.BUILD_TARGET=cordova",
    "build:cordova:prod": "rimraf cordova/www && webpack -p --progress --env.BUILD_MODE=prod --env.BUILD_TARGET=cordova",
    "serve:cordova": "rimraf cordova/www && webpack -d --progress --env.BUILD_TARGET=cordova --env.LIVE_RELOAD=true && webpack-dev-server -d --env.BUILD_TARGET=cordova --env.LIVE_RELOAD=true",
    "ioss": "npm run chrome && webpack-dev-server -d --progress",
    "ad": "cd cordova && cordova run android && cd ..",
    "adr": "cd cordova && cordova build android --release && cd ..",
    "ios": "cd cordova && cordova build ios && cd ..",
    "bcp": "npm run build:cordova:prod",
    "bpi": "npm run build:cordova:prod && npm run ios",
    "bpa": "npm run build:cordova:prod && npm run ad",
    "ba": "npm run build:cordova && npm run ad",
    "bcw": "rimraf cordova/www && webpack -d --progress --env.BUILD_TARGET=cordova  --watch",
    "aot-complie": "rimraf file_station/ng_aot && node_modules/.bin/ngc -p tsconfig.aot.json && git clean -fd",
    "sign": "sh ./command_tool/sign_android_app.sh",
    "signinstall": "sh ./command_tool/install_sign_app.sh",
    "test": "node ./command_tool/sign_android_app.js"
  },
  "dependencies": {
    "@angular/animations": "4.4.4",
